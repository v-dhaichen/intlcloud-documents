  "objects": {
    "AssignProjectRequest": {
      "document": "AssignProject请求参数结构体", 
      "members": [
        {
          "document": "实例ID列表，格式如：cmgo-p8vnipr5。与云数据库控制台页面中显示的实例ID相同", 
          "name": "InstanceIds"
        }, 
        {
          "document": "项目ID", 
          "name": "ProjectId"
        }
      ]
    }, 
    "AssignProjectResponse": {
      "document": "AssignProject返回参数结构体", 
      "members": [
        {
          "document": "返回的异步任务ID列表", 
          "name": "FlowIds"
        }, 
        {
          "document": "唯一请求 ID，每次请求都会返回。定位问题时需要提供该次请求的 RequestId。", 
          "name": "RequestId"
        }
      ]
    }, 
    "ClientConnection": {
      "document": "客户端连接信息，包括客户端IP和连接数", 
      "members": [
        {
          "document": "连接的客户端IP", 
          "name": "IP"
        }, 
        {
          "document": "对应客户端IP的连接数", 
          "name": "Count"
        }
      ]
    }, 
    "CreateDBInstanceHourRequest": {
      "document": "CreateDBInstanceHour请求参数结构体", 
      "members": [
        {
          "document": "实例内存大小，单位：GB", 
          "name": "Memory"
        }, 
        {
          "document": "实例硬盘大小，单位：GB", 
          "name": "Volume"
        }, 
        {
          "document": "副本集个数，1为单副本集实例，大于1为分片集群实例，最大不超过10", 
          "name": "ReplicateSetNum"
        }, 
        {
          "document": "每个副本集内从节点个数，目前只支持从节点数为2", 
          "name": "SecondaryNum"
        }, 
        {
          "document": "MongoDB引擎版本，值包括MONGO_3_WT 、MONGO_3_ROCKS和MONGO_36_WT", 
          "name": "EngineVersion"
        }, 
        {
          "document": "实例类型，GIO：高IO版；TGIO：高IO万兆", 
          "name": "Machine"
        }, 
        {
          "document": "实例数量，默认值为1, 最小值1，最大值为10", 
          "name": "GoodsNum"
        }, 
        {
          "document": "可用区信息，格式如：ap-guangzhou-2", 
          "name": "Zone"
        }, 
        {
          "document": "实例角色，支持值包括：MASTER-表示主实例，DR-表示灾备实例，RO-表示只读实例", 
          "name": "InstanceRole"
        }, 
        {
          "document": "实例类型，REPLSET-副本集，SHARD-分片集群", 
          "name": "InstanceType"
        }, 
